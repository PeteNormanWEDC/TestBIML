   <Connections >
    <Connection Name ="OLEB_Staging"
                ConnectionString="Data Source=DT121;Initial Catalog=RptgProd;
                   Provider=SQLOLEDB.1;Integrated Security=SSPI;Auto Translate=False;"/>
  </Connections>
  <Packages>
    <Package Name="Staging" ConstraintMode="Linear">
