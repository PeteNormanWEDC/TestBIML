       <ExecuteSQL Name="SQL - Execute SQL Query" ConnectionName="OLEB_Staging">
          <DirectInput>
              Select top 10 * from sys.tables                
          </DirectInput>
       </ExecuteSQL>
      </Tasks>        
    </Package>
  </Packages>
</Biml>